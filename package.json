  "license": "UNLICENSED",
  "private": true,
  "engines": {
    "node": ">=18.14.0"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development node ./src/index.js",
    "start:dev": "cross-env NODE_ENV=development nodemon ./src/index.js",
    "start:debug": "cross-env NODE_ENV=development nodemon --inspect ./src/index.js",
