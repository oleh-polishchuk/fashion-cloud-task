  "dependencies": {
    "compression": "^1.7.4",
    "config": "^3.3.9",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^6.0.1",
    "http-status": "^1.6.2",
    "joi": "^17.7.1",
    "mongoose": "^6.9.1",
    "pm2": "^5.2.2",
    "uuid": "^9.0.0",
    "xss-clean": "^0.1.1",
    "yn": "^5.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
